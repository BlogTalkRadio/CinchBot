    "jsdom": "0.5.6",
    "hubot": ">=2.3.4",
    "hubot-skype": "git://github.com/benjamine/hubot-skype.git",
    "hubot-irc": "^0.2.1"
  },
  "analyze": false,
  "devDependencies": {
    "hubot-scripts": "*"
