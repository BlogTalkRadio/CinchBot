{
  "name": "CinchBot",
  "preferGlobal": "false",
  "version": "0.0.1",
  "author": "Benjamin Eidelman <beneidel@gmail.com>",
  "description": "CinchCast hubot",
  "contributors": [],
  "scripts": {
    "update-scripts": "node ./update-scripts.js",
    "start": "node ./node_modules/coffee-script/bin/coffee ./node_modules/hubot/bin/hubot -a skype",
